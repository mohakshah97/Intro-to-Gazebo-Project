<?xml version="1.0" ?>

<robot name="my_rb1_robot">

    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="left_wheel">
        <material>Gazebo/Grey</material>
    </gazebo>
    <gazebo reference="right_wheel">
        <material>Gazebo/Grey</material>
    </gazebo>
    <gazebo reference="back_caster">
        <material>Gazebo/Grey</material>
    </gazebo>
    <gazebo reference="front_caster">
        <material>Gazebo/Grey</material>
    </gazebo>
    <!-- Link - base footprint -->
    <link name="base_footprint">
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0.3" rpy="0 0 0" />
            <inertia ixx="0.005625" ixy="0" ixz="0" iyy="0.005625" iyz="0" izz="0" />
        </inertial>

        <!-- <collision>
            <geometry>
                <cylinder radius = "0.05" length="0.5" />
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius = "0.05" length="0.5" />
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual> -->
    </link>
    <!-- Joint - base link -->
    <joint name="joint_base_footprint_base_link" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0.15" />
        <child link="base_link" />
        <parent link="base_footprint" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>    

    <!-- Link - base link -->
    <link name="base_link">
        <inertial>
            <mass value="19" />
            <origin xyz="0 0 0.15" rpy="0 0 0" />
            <inertia ixx="0.4625" ixy="0" ixz="0" iyy="0.4625" iyz="0" izz="0.625" />
        </inertial>

        <collision>
            <geometry>
                <cylinder radius = "0.25" length="0.3" />
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius = "0.25" length="0.3" />
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 0.5" />
            </material>
        </visual>
    </link>
    <!-- Joint - base_link / left wheel -->
    <joint name="joint_base_link_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.2 -0.15" />
        <child link="left_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Link - left wheel -->
    <link name="left_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00099" ixy="0" ixz="0" iyy="0.00099" iyz="0" izz="0.000312"/>
        </inertial>

        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.1" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.1" radius="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>
    <!-- Joint - base_link / right_wheel -->
    <joint name="joint_base_link_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 -0.2 -0.15" />
        <child link="right_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Link - right wheel -->
    <link name="right_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00099" ixy="0" ixz="0" iyy="0.00099" iyz="0" izz="0.000312"/>
        </inertial>

        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.1" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.1" radius="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>
    <!-- Joint - base_link / back_castor  -->
    <joint name="joint_base_back_castor" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0 -0.15" />
        <child link="back_castor" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

    <!-- Link - back_castor -->
    <link name="back_castor">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025"/>
        </inertial>

        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <sphere  radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <sphere  radius="0.025"/>
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 1" />
            </material>
        </visual>
    </link>
    <!-- Joint - base_link / front_castor -->
    <joint name="joint_base_link_front_castor" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0 -0.15" />
        <child link="front_castor" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

    <!-- Link - front_castor -->
    <link name="front_castor">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025"/>
        </inertial>

        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <sphere  radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <sphere  radius="0.025"/>
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 1" />
            </material>
        </visual>
    </link>
    <!-- Joint - base_link / front_laser -->
    <joint name="joint_base_link_front_laser" type="fixed">
        <origin rpy="-3.14 0 0" xyz="0.25 0 0.075" />
        <child link="front_laser" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Link - front_laser -->
    <link name="front_laser">
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.005625" ixy="0" ixz="0" iyy="0.005625" iyz="0" izz="0.03125"/>
        </inertial>

        <!-- <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <sphere  radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.025" radius="0.025"/>
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 1" />
            </material>
        </visual> -->
    </link>
</robot>